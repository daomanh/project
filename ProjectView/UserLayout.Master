                                </ul>
                            </li>
                        </ul>

                    </ul>
                </nav>
                <div class="content-wrapper">
                    <div class="container-fluid">

                        <div class="row">
                            <div class="col-md-12">

                                <%--main--%>
                                <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
                                </asp:ContentPlaceHolder>


                            </div>
                        </div>

                        <div class="row">
                            <div class="clearfix pt pb">
                                <div class="col-md-12">
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

            <!-- Loading Scripts -->


        </div>
